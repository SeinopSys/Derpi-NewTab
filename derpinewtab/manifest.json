{
	"manifest_version": 2,
	
	"name": "Derpi-New Tab",
	"description": "A new tab page featuring the latest pony wallpaper from Derpibooru.org",
	"version": "0.2.5",
	
	"permissions": [ "https://derpibooru.org/", "https://derpicdn.net/", "https://img0.derpicdn.net/", "https://img1.derpicdn.net/", "https://img2.derpicdn.net/", "https://img3.derpicdn.net/", "https://img4.derpicdn.net/", "https://img5.derpicdn.net/" ],
	
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png",
		"128": "icons/128.png",
		"256": "icons/256.png"
	},
	
	"chrome_url_overrides" : { "newtab": "index.html" }
}
